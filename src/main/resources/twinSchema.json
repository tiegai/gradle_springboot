{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^metastar:define:.*$"
    },
    "type": {
      "type": "string",
      "enum": ["define"]
    },
    "name": {
      "type": "string",
      "pattern": "^.*#.*$"
    }

  },
  "required": ["id","type","name"]
}